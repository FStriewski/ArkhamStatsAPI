generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model decks {
  id                Int      @id @default(autoincrement())
  did               String
  date              String
  investigator_code String
  investigator_name String
  xp                Int?
  slots             String[]
}
